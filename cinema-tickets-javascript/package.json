{
  "name": "cinema-tickets-javascript",
  "description": "This is a coding exercise which will allow you to demonstrate how you code and your approach to a given problem.",
  "version": "1.0.0",
  "engines": {
    "node": ">=16.15.1"
  },
  "type": "module",
  "scripts": {
    "start": "node ./src/app.js",
    "start:dev": "nodemon --watch ./src/ ./src/app.js",
    "test": "jest --coverage"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.20.2",
    "babel-jest": "^29.5.0",
    "chai": "^4.3.7",
    "jest": "28.1.1",
    "nodemon": "^2.0.22"
  },
  "jest": {
    "verbose": true,
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/thirdparty"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
